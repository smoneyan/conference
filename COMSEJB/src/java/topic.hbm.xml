<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
  <class name="coms.datatransfer.TopicDTO" table="topic">
    <meta attribute="class-description"> Topic details </meta>
    <id name="topicId" type="int" column="tid">
      <generator class="native" />
    </id>
    <property name="title" not-null="true" length="30" column="title" />
    </class>
</hibernate-mapping>
